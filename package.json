  "dependencies": {
    "@react-pdf/renderer": "^2.0.21",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-validator": "^6.12.0",
    "gapi-client": "^0.0.3",
    "googleapis": "^39.2.0",
    "html2canvas": "^1.3.2",
